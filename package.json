{
  "name": "reason-store",
  "version": "0.1.0",
  "description": "ReasonML bindings for store.js: a localStorage wrapper for all browsers without using cookies or flash. Uses localStorage, globalStorage, and userData behavior under the hood",
  "scripts": {
    "build": "bsb -make-world",
    "start": "bsb -make-world -w -ws _ ",
    "clean": "bsb -clean-world",
    "test": "echo \"Error: no test specified\" && exit 1",
    "example": "moduleserve ./example --port 8000",
    "release": "npmpub"
  },
  "keywords": [
    "BuckleScript",
    "ReasonML",
    "localstorage",
    "bindings",
    "rescript"
  ],
  "license": "MIT",
  "author": "Alain Armand",
  "bugs": {
    "url": "http://github.com/idkjs/reason-store/issues"
  },
  "homepage": "https://github.com/idkjs/reason-store#readme",
  "repository": "https://github.com/idkjs/reason-store.git",
  "dependencies": {
    "store": "^2.0.12"
  },
  "devDependencies": {
    "bs-platform": "^8.3.3",
    "moduleserve": "^0.9.0",
    "npmpub": "^5.0.0",
    "tape": "^5.0.1",
    "tinytest": "^1.1.5"
  },
  "peerDependencies": {
    "bs-platform": "*",
    "store": "^2.0.12"
  }
}
